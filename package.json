{
  "name": "ado-git-repo-seeder",
  "version": "1.1.0",
  "description": "Configurable tool to seed realistic, multi-user pull-request activity on Azure DevOps",
  "main": "dist/cli.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node --loader ts-node/esm src/cli.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:strict": "eslint src --ext .ts --max-warnings 73",
    "format": "prettier --write \"src/**/*.ts\" \"scripts/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"scripts/**/*.ts\"",
    "sync-badges": "npx tsx scripts/sync-badges.ts",
    "prepare": "husky"
  },
  "keywords": [
    "azure-devops",
    "seeder",
    "testing",
    "git"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^13.1.3",
    "@types/node": "^20.11.0",
    "@types/seedrandom": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vitest/coverage-v8": "^4.0.17",
    "eslint": "^8.56.0",
    "husky": "^9.1.7",
    "prettier": "^3.5.0",
    "semantic-release": "^25.0.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.17"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "dotenv": "^16.3.1",
    "seedrandom": "^3.0.5",
    "zod": "^3.22.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}